---
title: Home
published: true
listed: true
---
!!! 5
%html{ :lang => '{{ locale }}' }

  %head
    -# To add a subtitle to the page, place it in the "subtitle" block.
    %title {{ page.title | escape }}{% block 'subtitle' %}{% endblock %} - {{ site.name | escape }}

    %meta{ :charset => 'utf-8' }
    %meta{ :name    => 'keywords',
           :content => '{{ site.meta_keywords | escape }}{% if page.meta_keywords %} {{ page.meta_keywords | escape }}{% endif %}{% if entry and entry.meta_keywords %} {{ entry.meta_keywords | escape }}{% endif %}' }
    %meta{ :name    => 'description',
           :content => '{% if entry and entry.meta_description %}{{ entry.meta_description | escape }}{% elsif page.meta_description %}{{ page.meta_description | escape }}{% else %}{{ site.meta_description | escape }}{% endif %}' }

    %link{ :rel => 'stylesheet', :type => 'text/css', :href => '{{ "semantic-ui.min.css" | stylesheet_url | escape }}' }
    %link{ :rel => 'stylesheet', :type => 'text/css', :href => '{{ "application.css" | stylesheet_url | escape }}' }

    {% block 'feed' %}
    {% with_scope fullpath: 'feed' %}{% assign feed_page = site.pages.first %}{% endwith_scope %}
    %link{ :rel => 'alternate', :type => 'application/atom+xml', :href => '{% path_to feed_page %}' }
    {% endblock %}

    %script{ :type => 'text/javascript', :src => '{{ "jquery-1.11.3.min.js" | javascript_url | escape }}' }
    -# Pages can add additional scripts by placing them in the "javascripts" block.
    {% block 'javascripts' %}{% endblock %}

  %body
    #container
      #header
        {% include 'nav' %}
        {% block 'breadcrumbs' %}{% endblock %}
        #heading.ui.bottom.segment
          .ui.page.grid
            .sixteen.wide.column
              {% block 'heading' %}
              {% if parent %}
              {% include 'heading' %}
              {% else %}
              .ui.header {{ site.name | escape }}
              {% endif %}
              {% endblock %}

      #content
        .ui.page.grid
          .sixteen.wide.column
            -# All primary content goes in the "content" block.
            {% block 'content' %}
            .ui.segment
