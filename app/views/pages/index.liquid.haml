      #footer
        #related
          .ui.centered.grid
            .fourteen.wide.column
              {% block 'related' %}
              {% include 'latest_posts' %}
              {% include 'latest_updates' %}
              {% include 'featured_projects' %}
              {% include 'featured_tutorials' %}
              {% endblock %}

        .ui.inverted.borderless.segment
          .ui.stackable.grid
            {% block 'footer' %}
            {% capture 'logo_url' %}{% editable_file 'logo', hint: 'Site-wide logo displayed in the page footer.', fixed: true %}{% endeditable_file %}{% endcapture %}
            #logo.center.aligned.four.wide.column
              %a{ :href => '/' }
                %img.ui.center.aligned{ :src => '{% if logo_url %}{{ logo_url | resize: "240x100#" | escape }}{% else %}{{ "logo_placeholder.png" | theme_image_url | escape }}{% endif %}', :alt => '{{ site.name | escape }}' }

            #site-nav.two.wide.column
              %span Site Links
              %hr
              .ui.link.list
                {% with_scope depth: 1 %}
                {% for p in site.pages %}
                {% if p.listed? and p.published? %}
                %a.item{ :href => '{% path_to p %}' } {{ p.title | escape }}
                {% endif %}
                {% endfor %}
                {% endwith_scope %}

            #network.two.wide.column
              {% if contents.site_links.size > 0 %}
              %span Network
              %hr
              .ui.link.list
                {% for link in contents.site_links %}
                %a.item{ :href => '{{ link.url | escape }}' } {{ link.name | escape }}
                {% endfor %}
              {% endif %}

            .two.wide.column

            .six.wide.column
              {% editable_text 'footer_text', format: 'html', rows: 3, hint: 'Footer text.', fixed: true %}<p>Site powered by <a href="http://www.locomotivecms.com/">LocomotiveCMS</a>.<p>{% endeditable_text %}
              %p &copy; {{ today | date: '%Y' }} {{ site.name | escape }}
            {% endblock %}

    {% include 'disqus_comment_links' %}
    {% capture 'tracking_id' %}{% editable_text 'google_analytics_tracking_id', format: 'raw', rows: 1, hint: 'UA-######', fixed: true %}{% endeditable_text %}{% endcapture %}
    %script{ :type => 'text/javascript' }
      :plain
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', '{{ tracking_id | escape }}', 'auto');
        ga('send', 'pageview');
    -# Pages can run custom scripts after the page is loaded by including them in the "scripts" block.
    {% block 'scripts' %}{% endblock %}
