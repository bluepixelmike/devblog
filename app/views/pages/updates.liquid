---
title: Updates
listed: true
published: true
---
{% extends 'index' %}

{% block 'html_head' %}  {{ '/feed/updates' | auto_discovery_link_tag }}{% endblock %}

{% block 'header' %}
    <i class="bullhorn icon"></i>
    <span class="content">{{ page.title | escape }}</span>
{% endblock %}

{% block 'body/main' %}
<!-- update-list -->

<div class="ui red stacked segment">
  <div class="ui divided items">
{% paginate contents.project_updates by 10 %}
{% for update in paginate.collection %}
{% include 'update_list_item' with update %}
{% endfor %}
{% include 'pagination' with paginate, color: 'red' %}
{% endpaginate %}
  </div>
</div>

<!-- /update-list -->
{% endblock %}
