---
title: Updates
listed: true
published: true
---
{% extends 'index' %}
{% block 'html_head' %}    {{ '/feed/updates' | auto_discovery_link_tag }}{% endblock %}

{% block 'header' %}
    <i class="bullhorn icon"></i>
    <div class="content">
      {{ page.title | escape }}
    </div>
{% endblock %}

{% block 'body/main' %}

<!-- update-list -->

<div class="ui red stacked segment">
  <div class="ui divided items">
{% paginate contents.project_updates by 10 %}
{% for update in paginate.collection %}
{% include 'update_list_item' with update %}
{% endfor %}
{% include 'pagination' with paginate %}
{% endpaginate %}
  </div>
</div>

<!-- /update-list -->

{% endblock %}
